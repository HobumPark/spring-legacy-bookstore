<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration 
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
 	<settings>
        <!-- 카멜 케이스 VO 매핑 -->       
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    
	<typeAliases>
            <typeAlias type="spring.com.dto.MemberVO" alias="memberVO"/>
            <typeAlias type="spring.com.dto.BookVO" alias="BookVO"/>
            <typeAlias type="spring.com.dto.BasketVO" alias="BasketVO"/>
            <typeAlias type="spring.com.dto.NoticeBoardVO" alias="NoticeBoardVO"/>
            <typeAlias type="spring.com.dto.UserBoardVO" alias="UserBoardVO"/>
            <typeAlias type="spring.com.dto.UserBoardComment" alias="UserBoardComment"/>
            <typeAlias type="spring.com.dto.NestedComment" alias="NestedComment"/>
    </typeAliases>
      
</configuration>

	